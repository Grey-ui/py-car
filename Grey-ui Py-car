import RPi.GPIO as GPIO

GPIO.setup(2, GPIO.OUT)
GPIO.setup(5, GPIO.OUT)
GPIO.setup(3, GPIO.OUT)
GPIO.setup(4, GPIO.OUT)

def forward():
    GPIO.output(3, 1)
    GPIO.output(4, 1)

def backward():
    GPIO.output(2, 1)
    GPIO.output(5, 1)

def right():
    GPIO.output(2, 1)
    GPIO.output(4, 1)

def left():
    GPIO.output(3, 1)
    GPIO.output(5, 1)

